{
  "cron": [
    {
      "command": "python task.py",
      "schedule": "0 5 * * *"
    }
  ],
  "formations": {
    "web": {
      "autoscaling": {
        "type": "memory",
        "metadata": {
          "type": "Utilization",
          "value": "60"
        }
      }
    }
  },
  "healthchecks": {
    "web": [
      {
        "attempts": 2,
        "content": "python/http.server",
        "name": "check-1",
        "path": "/",
        "timeout": 5,
        "type": "startup",
        "wait": 2
      }
    ]
  }
}
